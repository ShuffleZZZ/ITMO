{
  "attr-no-dup": true,
  "attr-validate": true,
  "doctype-first": true,
  "doctype-html5": true,
  "head-valid-content-model": true,
  "html-valid-content-model": true,
  "id-no-dup": true,
  "img-req-alt": "allownull",
  "img-req-src": true,
  "label-req-for": true,
  "tag-close": true,
  "title-no-dup": true,

  "fig-req-figcaption": true,
  "focusable-tabindex-style": true,
  "input-radio-req-name": true,
  "input-req-label": true,
  "table-req-caption": true,
  "table-req-header": true,
  "tag-name-match": true,

  "attr-bans": [
    "align",
    "background",
    "bgcolor",
    "border",
    "frameborder",
    "longdesc",
    "marginwidth",
    "marginheight",
    "scrolling",
    "style",
